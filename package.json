{
   "name": "hamburger-menu",
   "version": "0.4.1",
   "description": "☰ A responsive CSS solution for a hamburger menu ☰",
   "license": "MIT",
   "type": "module",
   "main": "dist/hamburger-menu.js",
   "jsdelivr": "dist/hamburger-menu.min.js",
   "style": "dist/hamburger-menu.min.css",
   "files": [
      "dist"
   ],
   "repository": "github:center-key/hamburger-menu",
   "homepage": "https://github.com/center-key/hamburger-menu",
   "bugs": "https://github.com/center-key/hamburger-menu/issues",
   "docs": "https://github.com/center-key/hamburger-menu#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "css",
      "hamburger-button",
      "hamburger-menu",
      "hamburger",
      "menu",
      "mobile-first",
      "mobile-web",
      "responsive",
      "ui"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "jquery": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "step:01": "rimraf build dist **/.DS_Store",
      "step:02": "jshint . --exclude node_modules,build,dist",
      "step:05": "gulp minify-css",
      "step:06": "cpy src/*.js build --flat=true",
      "step:07": "gulp minify-js",
      "step:08": "add-dist-header build dist --delimiter=☰",
      "step:09": "cpy dist/hamburger-menu.css docs --flat=true",
      "step:10": "cpy dist/hamburger-menu.js docs --flat=true",
      "step:11": "w3c-html-validator",
      "pretest": "npm-run-all step:*",
      "test": "mocha spec/*.spec.js",
      "interactive": "npm test && browser-sync docs --watch"
   },
   "dependencies": {
      "jquery": "~3.6"
   },
   "devDependencies": {
      "@babel/core": "~7.18",
      "@babel/preset-env": "~7.18",
      "@fortawesome/fontawesome-free": "~6.1",
      "add-dist-header": "~0.1",
      "assert-deep-strict-equal": "~1.0",
      "babel-minify": "~0.5",
      "browser-sync": "~2.27",
      "cpy-cli": "~4.1",
      "cssnano": "~5.1",
      "dna.js": "~2.0",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-postcss": "~9.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.1",
      "gulp-size": "~4.0",
      "jshint": "~2.13",
      "mocha": "~10.0",
      "npm-run-all2": "~5.0",
      "postcss": "~8.4",
      "rimraf": "~3.0",
      "w3c-html-validator": "~1.0",
      "web-ignition": "~1.5"
   }
}
